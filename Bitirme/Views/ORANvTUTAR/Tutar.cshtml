@using Bitirme.Models
@model List<GenelTutar>
@{
    ViewBag.Title = "Tutar";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col"></th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MaliyetId </td>
                <td>@item.Geneltutar1 </td>
                <td>@item.HesapKodu @item.HesapAd </td>
                <td>@item.Tutar </td>
                <td>@item.FaaliyetAd </td>
                <td>@item.KMEAd</td>


            </tr>
        }
    </tbody>
</table>

